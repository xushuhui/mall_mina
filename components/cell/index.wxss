.container{
    height: 60rpx;
    border: 2rpx solid #333;
    padding: 2rpx;
    box-sizing: border-box;
}
.inner-container{
    height: 100%;
    padding-left: 15rpx;
    padding-right: 15rpx;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    align-items: center;
    font-size: 24rpx;
}
.forbidden{
    border: 2rpx dashed #bbb;
    color: #bbb;
}
.s-outer{
    border: 2rpx dashed #157658;
    color: #fff;
}
.s-inner{
    background-color: #157658;
}
